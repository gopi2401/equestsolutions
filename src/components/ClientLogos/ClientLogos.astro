---
// import "./ClientLogos.scss";
import SliderAstro from "../SliderAstro";

const { title } = Astro.props;
---

<section class="client-logos pt-0">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="heading_wrapper">
          <h2 class="sec_heading sec_heading-medium with_border">
            {title}
          </h2>
        </div>
      </div>
    </div>
  </div>
  <div class="container-fluid">
    <SliderAstro client:only="react"/>
  </div>
</section>
